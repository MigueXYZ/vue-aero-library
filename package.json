{
  "name": "@miguexyz/vue-aero",
  "version": "0.1.5",
  "description": "Dark-Aero UI components for Vue 3",
  "keywords": [
    "vue3",
    "components",
    "glassmorphism",
    "ui",
    "library"
  ],
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/MigueXYZ/vue-aero-library",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MigueXYZ/vue-aero-library.git"
  },
  "private": false,
  "files": [
    "dist"
  ],
  "main": "dist/vue-aero.umd.js",
  "module": "dist/vue-aero.es.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/vue-aero.css",
  "exports": {
    "./dist/types/index.d.ts": {
      "types": "./dist/types/index.d.ts"
    },
    ".": {
      "import": "./dist/vue-aero.es.js",
      "require": "./dist/vue-aero.umd.js"
    },
    "./style": "./dist/vue-aero.css",
    "./dist/vue-aero.css": "./dist/vue-aero.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.lib.ts",
    "docs:dev": "vite --config vite.config.docs.ts",
    "docs:build": "vite build --config vite.config.docs.ts --emptyOutDir",
    "docs:preview": "vite preview --config vite.config.docs.ts"
  },
  "peerDependencies": {
    "prismjs": "^1.30.0",
    "vue": "^3.3.0 || ^3.4.0 || ^3.5.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "vite": "^6.3.5",
    "vue-router": "^4.5.1"
  },
  "sideEffects": false
}
